{
  "name": "angular-webpack-bootstrap-seed",
  "version": "1.0.0",
  "description": "testing angular webpack integration",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8011",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:aot": "webpack --config webpack.config.aot.js --progress",
    "serve:aot": "lite-server -c=bs-config.aot.js",
    "build:uni": "webpack --config webpack.config.uni.js --progress",
    "serve:uni": "node src/dist/server.js"
  },
  "license": "MIT",
