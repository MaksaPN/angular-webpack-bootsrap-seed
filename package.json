    "@angular/platform-browser-dynamic": "~4.1.3",
    "@angular/router": "~4.1.3",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "ngx-bootstrap": "^1.7.1",
    "rxjs": "5.4.0",
    "zone.js": "^0.8.11"
  },
